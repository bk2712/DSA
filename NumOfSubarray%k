int findSubarray(){
        unordered_map<int,int> mp;
        int n= nums.size(),sum=0,cnt=0;
        for(int i=0;i<n;i++) nums[i]%= k;
        mp[0]= 1;
        for(int i=0;i<n;i++){
            sum+=nums[i];
            sum%=k;
            if(mp.find(sum)!=mp.end()){
                cnt+= mp[sum];
            }
            mp[sum]++;
        }
        return cnt;
}
