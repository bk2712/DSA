int sol(string s1,string s2,int i,int j,int m,int n,vector<vector<int>> &dp){
        if(i==m || j==n) return 0;
        if(dp[i][j]!= -1) return dp[i][j];
        if(s1[i]==s2[j]) return dp[i][j]= 1+sol(s1,s2,i+1,j+1,m,n,dp);
        return dp[i][j]= max(sol(s1,s2,i+1,j,m,n,dp),sol(s1,s2,i,j+1,m,n,dp));
    }
    int lcs(int x, int y, string s1, string s2)
    {
        vector<vector<int>> dp(x,vector<int> (y,-1));
        return sol(s1,s2,0,0,x,y,dp);
    }
