class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        if(!head || !head->next) return head;
        ListNode* newh= NULL;
        while(head){
            ListNode* next= head->next;
            head->next= newh;
            newh= head;
            head= next;
        }
        return newh;
    }
};
